{
    "manifest_version": 3,
    "name": "Insta-MOD",
    "description": "To save Image by Rclick.",
    "version": "0.1",
    "icons":
    {
        "512": "icon_512.png"
    },
    "author": "Rasan",
    "developer":
    {
        "name": "Rasan"
    },
    "mod":
    {
        "schema_version": 1,
        "payload":
        { 
            "page_styles":
            [
                {
                    "matches":
                    [
                        "https://*.instagram.com/*"
                    ],
                    "css":
                    [
                        "css/main.css"
                    ]
                }
            ]
        }
    }
}